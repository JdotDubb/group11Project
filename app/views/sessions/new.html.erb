<section>
    <p id="alert"><%= alert %></p>
    <h1>cNotes - Login</h1>
    
    <%= form_tag sessions_path do |form| %>
    
        <br>
        <br>
        <%= label_tag :email %>
        <%= text_field_tag :email %>
    
        <br>
        <br>
        <%= label_tag :password %>
        <%= password_field_tag :password %>

        <br>
        <br>
        <%= submit_tag "Login!" %>


    
    <% end %>
    </section>