<h1>Software Engineering</h1>
<br>
<br>
<h2>Class Notes</h2>
<br>
<table>
    <tr>
        <th>Title of Note</th>
    </tr>
    <% @notes.each do |note| %>
    <tr>
        <td><%= note.title %></td>
        <td><%= link_to 'Show', note_path(note) %></td>
        <td><%= link_to 'Edit', edit_note_path(note) %></td>
        
        <td><%= link_to 'Delete', 
                                    note_path(note),
                                    method: :delete,
                                    data: {confirm: 'Do you want to delete?'} %></td>
        
    </tr>
    <% end %>
</table>
<br>
<%= link_to 'Back', notes_class1_path %>