<h1>Your note dashboard</h1>
<br>
<br>
<strong>Click here to make a new notes.</strong>
<br>
<%= link_to 'New note', new_note_path %>
<Br>
<br>
<h2>My Notes</h2>
<table>
    <tr>
        <th>Title of Note</th>
        <th>Note</th>
    </tr>
    <% @note.each do |note| %>
    <tr>
        <td><%= note.title %></td>
        <td><%= note.text %></td>
        <td><%= link_to 'Show', note_path(note) %></td>
    </tr>
    <% end %>
</table>
